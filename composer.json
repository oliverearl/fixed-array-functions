{
    "name": "petrobolos/fixed-array-functions",
    "description": "This is my package fixed-array-functions",
    "keywords": [
        "laravel",
        "splfixedarray",
        "array",
        "collection"
    ],
    "homepage": "https://github.com/petrobolos/fixed-array-functions",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Earl",
            "email": "oliver.earl@petrobolos.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0 || ^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^6.0 || ^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Petrobolos\\FixedArray\\": "src",
            "Petrobolos\\FixedArray\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Petrobolos\\FixedArray\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Petrobolos\\FixedArray\\FixedArrayFunctionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
